{
  "name": "oceanix-f",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:ssl": "ng serve --ssl --ssl-cert ssl/cert.pem --ssl-key ssl/key.pem --proxy-config proxy.conf.json",
    "dev:subdomain": "node scripts/start-with-subdomain.js",
    "dev:techcorp": "ng serve --proxy-config proxy.conf.json",
    "dev:acme": "node scripts/start-with-subdomain.js acme",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
